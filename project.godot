config_version=4

_global_script_classes=[ {
"base": "Position2D",
"class": "Dot",
"language": "GDScript",
"path": "res://ScenesAndScripts/MazePathDot.gd"
} ]
_global_script_class_icons={
"Dot": ""
}

[application]

config/name="MazeGenerator"
run/main_scene="res://ScenesAndScripts/MazeGenerator.tscn"
